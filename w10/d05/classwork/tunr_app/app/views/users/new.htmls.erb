<div class="form">
  <h1> Not a member yet </h1>
  <%= form_for @user do |f| %> <br>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <%= f.label :password_confirmation %><br>
  <%= f.password_field :password_confirmation %><br>
  <%= f.submit "sign up" %> <br>
  <%  end %>
</div>
